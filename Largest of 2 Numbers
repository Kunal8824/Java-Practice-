import java.util.*;
public class Largestof2number{
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);

        int a = s.nextInt();
        int b = s.nextInt();

        if(a > b){
            System.out.println("A is greater.");
        }
        else{
            System.out.println("B is greater.");
        }
    }
}
