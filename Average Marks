import java.util.*;
public class AverageMarks{
    public static void main(String[] args){
        
    
        Scanner s = new Scanner(System.in);
        String greeting = s.nextLine();
        int a = s.nextInt();
        int b = s.nextInt();
        int c = s.nextInt();
        
        int avg = (a + b + c)/3;
        System.out.println(greeting);
        System.out.println(avg);
    }
}
