import java.util.*;
public class APSeries{
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        int x = s.nextInt();
        int n = 1;
        int count = 1;
        while(count <= x){
            int d = 3 * n + 2;
            if(d % 4 != 0){
                System.out.println(d);
                count = count + 1; 
            }
            n = n + 1;
        }
    }    
}
