import java.util.*;
public class Multiplicationtable{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);

        int n = scan.nextInt();
        int i = 1;

        while(i <= 10){
            System.out.println(n + "*" + i + "=" + n*i);
            i = i + 1;
        }
    }
}
