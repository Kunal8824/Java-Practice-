import java.util.Scanner;
public class bonus{
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        int salary = s.nextInt();
        int year = s.nextInt();

        double bonus = salary / 20;

        if(year > 5){
            System.out.println(bonus);
        }
        else{
            System.out.println(salary);
        }
    }
}
